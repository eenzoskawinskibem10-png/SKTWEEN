tpCorner.CornerRadius = UDim.new(0, 10)
tpCorner.Parent = tpBtn

-- BOT√ÉO TP PAREDE (NOME MUDADO)
local wallTPBtn = Instance.new("TextButton")
wallTPBtn.Size = UDim2.new(0.9, 0, 0, 45)
wallTPBtn.Position = UDim2.new(0.05, 0, 0, 120)
wallTPBtn.BackgroundColor3 = Color3.fromRGB(110, 20, 160)
wallTPBtn.Text = "üß± TP parede"  -- ‚úÖ NOME MUDADO
