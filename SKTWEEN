wallTPCorner.CornerRadius = UDim.new(0, 10)
wallTPCorner.Parent = wallTPBtn

local statusLabel = Instance.new("TextLabel")
statusLabel.Size = UDim2.new(0.9, 0, 0, 25)
statusLabel.Position = UDim2.new(0.05, 0, 1, -30)
statusLabel.BackgroundTransparency = 1
statusLabel.Text = "üî• Carregando..."
statusLabel.TextColor3 = Color3.fromRGB(180, 160, 255)
statusLabel.Font = Enum.Font.Gotham
statusLabel.TextSize = 15
statusLabel.TextXAlignment = Enum.TextXAlignment.Left
statusLabel.Parent = contentFrame

-- * TELEPORTE PAREDE 14 CHUNKS *
local function tpParede14Chunks()
    if not humanoidRootPart then return end
    
    local currentY = humanoidRootPart.Position.Y
    local rayOrigin = humanoidRootPart.Position
    local rayDirection = humanoidRootPart.CFrame.LookVector * wallRayDistance
    
    local raycastParams = RaycastParams.new()
    raycastParams.FilterType = Enum.RaycastFilterType.Blacklist
    raycastParams.FilterDescendantsInstances = {character}
    
    local raycastResult = Workspace:Raycast(rayOrigin, rayDirection, raycastParams)
    
    if raycastResult then
        local hitPosition = raycastResult.Position
        local tpPosition = hitPosition + (rayDirection.Unit * 3)
        tpPosition = Vector3.new(tpPosition.X, currentY, tpPosition.Z)
        
        humanoidRootPart.CFrame = CFrame.new(tpPosition, tpPosition + humanoidRootPart.CFrame.LookVector)
        statusLabel.Text = "üß± TP parede!"
        statusLabel.TextColor3 = Color3.fromRGB(0, 255, 150)
        
    else
        local tpPosition = rayOrigin + (rayDirection.Unit * wallRayDistance)
        tpPosition = Vector3.new(tpPosition.X, currentY, tpPosition.Z)
        
        humanoidRootPart.CFrame = CFrame.new(tpPosition, tpPosition + humanoidRootPart.CFrame.LookVector)
        statusLabel.Text = "üöÄ TP 14 chunks!"
        statusLabel.TextColor3 = Color3.fromRGB(100, 255, 255)
    end
    
    local originalSize = wallTPBtn.Size
    TweenService:Create(wallTPBtn, TweenInfo.new(0.2, Enum.EasingStyle.Back), {
        Size = UDim2.new(0.95, 0, 0, 52),
        Position = UDim2.new(0.025, 0, 0, 114)
    }):Play()
    task.wait(0.2)
    TweenService:Create(wallTPBtn, TweenInfo.new(0.15, Enum.EasingStyle.Back), {
        Size = originalSize,
        Position = UDim2.new(0.05, 0, 0, 120)
    }):Play()
end

-- Resto do c√≥digo (intro, drag, save, tp, minimize, close) permanece ID√äNTICO
local function introAnimation()
    statusLabel.Text = "üéâ SK TWEEN carregado!"
    TweenService:Create(mainFrame, TweenInfo.new(0.8, Enum.EasingStyle.Back, Enum.EasingDirection.Out), {
        Position = UDim2.new(0.5, -140, 0.4, -105)
    }):Play()
    TweenService:Create(header, TweenInfo.new(0.6, Enum.EasingStyle.Quad, Enum.EasingDirection.InOut, 0, true), {
        Size = UDim2.new(1, 0, 0, 45)
    }):Play()
end

local dragging = false
local dragInput
local dragStart
local startPos

local function updateInput(input)
    local Delta = input.Position - dragStart
    TweenService:Create(mainFrame, TweenInfo.new(0.1, Enum.EasingStyle.Quad), {
        Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + Delta.X, startPos.Y.Scale, startPos.Y.Offset + Delta.Y)
    }):Play()
end

header.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
        dragging = true
        dragStart = input.Position
        startPos = mainFrame.Position
        TweenService:Create(mainFrame, TweenInfo.new(0.2), {
            Size = UDim2.new(0, 285, 0, 215)
        }):Play()
        input.Changed:Connect(function()
            if input.UserInputState == Enum.UserInputState.End then
                dragging = false
                TweenService:Create(mainFrame, TweenInfo.new(0.2, Enum.EasingStyle.Back), {
                    Size = UDim2.new(0, 280, 0, 210)
                }):Play()
            end
        end)
    end
end)

header.InputChanged:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
        dragInput = input
    end
end)

UserInputService.InputChanged:Connect(function(input)
    if input == dragInput and dragging then
        updateInput(input)
    end
end)

local function updateCharacter()
    character = player.Character or player.CharacterAdded:Wait()
    humanoidRootPart = character:WaitForChild("HumanoidRootPart")
end

updateCharacter()
player.CharacterAdded:Connect(updateCharacter)

saveBtn.MouseButton1Click:Connect(function()
    if humanoidRootPart then
        savedPosition = humanoidRootPart.CFrame
        statusLabel.Text = "‚úÖ Posi√ß√£o salva!"
        statusLabel.TextColor3 = Color3.fromRGB(0, 255, 100)
        local originalSize = saveBtn.Size
        TweenService:Create(saveBtn, TweenInfo.new(0.2, Enum.EasingStyle.Back), {
            Size = UDim2.new(0.95, 0, 0, 52),
            Position = UDim2.new(0.025, 0, 0, 4)
        }):Play()
        task.wait(0.2)
        TweenService:Create(saveBtn, TweenInfo.new(0.2, Enum.EasingStyle.Back), {
            Size = originalSize,
            Position = UDim2.new(0.05, 0, 0, 10)
        }):Play()
    else
        statusLabel.Text = "‚ùå Sem personagem"
        statusLabel.TextColor3 = Color3.fromRGB(255, 100, 100)
    end
end)

tpBtn.MouseButton1Click:Connect(function()
    if savedPosition and humanoidRootPart then
        humanoidRootPart.CFrame = savedPosition
        statusLabel.Text = "üöÄ Teleportado!"
        statusLabel.TextColor3 = Color3.fromRGB(100, 255, 255)
        local originalSize = tpBtn.Size
        TweenService:Create(tpBtn, TweenInfo.new(0.2, Enum.EasingStyle.Back), {
            Size = UDim2.new(0.95, 0, 0, 52),
            Position = UDim2.new(0.025, 0, 0, 59)
        }):Play()
        task.wait(0.2)
        TweenService:Create(tpBtn, TweenInfo.new(0.2, Enum.EasingStyle.Back), {
            Size = originalSize,
            Position = UDim2.new(0.05, 0, 0, 65)
        }):Play()
    else
        statusLabel.Text = "‚ùå Sem posi√ß√£o salva"
        statusLabel.TextColor3 = Color3.fromRGB(255, 100, 100)
    end
end)

wallTPBtn.MouseButton1Click:Connect(tpParede14Chunks)

minimizeBtn.MouseButton1Click:Connect(function()
    minimized = not minimized
    if minimized then
        TweenService:Create(contentFrame, TweenInfo.new(0.3, Enum.EasingStyle.Quad), {
            Size = UDim2.new(1, -20, 0, 0)
        }):Play()
        task.wait(0.3)
        contentFrame.Visible = false
        TweenService:Create(mainFrame, TweenInfo.new(0.3, Enum.EasingStyle.Back), {
            Size = UDim2.new(0, 280, 0, 40)
        }):Play()
        minimizeBtn.Text = "+"
    else
        contentFrame.Visible = true
        TweenService:Create(mainFrame, TweenInfo.new(0.3, Enum.EasingStyle.Back), {
            Size = UDim2.new(0, 280, 0, 210)
        }):Play()
        task.wait(0.3)
        TweenService:Create(contentFrame, TweenInfo.new(0.3), {
            Size = UDim2.new(1, -20, 1, -50)
        }):Play()
        minimizeBtn.Text = "-"
    end
end)

closeBtn.MouseButton1Click:Connect(function()
    TweenService:Create(mainFrame, TweenInfo.new(0.4, Enum.EasingStyle.Back, Enum.EasingDirection.In), {
        Size = UDim2.new(0, 0, 0, 0),
        Position = UDim2.new(0.5, 0, 0.5, 0)
    }):Play()
    task.wait(0.4)
    screenGui:Destroy()
end)

task.wait(0.1)
introAnimation()
statusLabel.Text = "üß± TP parede pronto! ‚ú®"
